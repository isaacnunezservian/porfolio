    .u-image.u-image-default.u-image-1,
    .u-menu.u-menu-1 {
        display: inline-block; /* Asegura que se comporten como elementos en línea */
        vertical-align: middle; /* Alineación vertical */
    }
  
    .u-image.u-image-default.u-image-1 {
        margin-right: 50px !important; /* Espacio entre el logo y la barra */
    }
  
    .u-menu.u-menu-1 {
        margin-left: 0 !important; /* Resetea cualquier margen no deseado */
    }
  }
  
  

